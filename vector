#include <iostream>
#include <vector>

  using namespace std;
  //problem 1
  void inputM(int maxtrix[][100],int N, int M)
  {
    for(int i=0;i<N;i++)
    { for(int j=0;j<M;j++)
    {
      cin>>maxtrix[i][j];
    }}}
  //problem 2
  void Displaym(int maxtrix[][100],int N,int M) {

    for(int i=0;i<N;i++)
    { for(int j=0;j<M;j++)
    {
      cout<<maxtrix[i][j]<<" ";
    }
      cout<<endl; }
  }
  //problem 3
  void Sum(int matrix[][100],int N, int M)
  { int sum=0;
    for(int i=0;i<N;i++)
    {
      for(int j=0;j<M;j++)
      {
        sum+=matrix[i][j];}}
    cout<<sum<<endl;}
//4 problem
  void max(int matrix[][100],int N, int M)
  {
    for(int i=0;i<N;i++)
    {
      int max=matrix[i][0];
      for(int j=0;j<M;j++)
        if(matrix[i][j]>max) max=matrix[i][j];
      cout<<max<<endl;
    }}
// 5 problem
void Sum1(int matrix[][100],int N, int M)
  {
    for(int i=0;i<N;i++)
    { int sum=0;
      for(int j=0;j<M;j++) {
        sum+=matrix[i][j];}
    cout<<sum<<endl; }
   }
 //6 problem
void diagonal(int matrix[][100], int N){
  int sum=0;
  for (int i=0;i<N;i++) {
    sum += matrix[i][i];
  }
    cout<<sum<<endl;
  } // 7 problem
void Transpose(int matrix[][100],int N, int M) {
    for (int i=0;i<M;i++) {
      for (int j=0;j<N;j++) {
        cout<<matrix[j][i]<<" ";
      }
     cout<<endl; }
  } // 8 problem
void addition(int matrix[][100],int matrix1[][100],int result[][100],int N,int M) {

    for (int i=0;i<N;i++) {
      for (int j=0;j<M;j++) {
        result[i][j]=matrix[i][j]+matrix1[i][j];
      }}
for (int i=0;i<N;i++) {
  for (int j=0;j<M;j++) {
    cout<<result[i][j]<<" ";}
    cout<<endl; }}



//10
void Multiplication(int a[][100],int b[][100],int c[][100],
                    int n, int m,int w)
{ for(int i=0;i<n;i++) {
  for (int j=0;j<w;j++) {
     c[i][j]=0;
  for (int k=0;k<m;k++){
    c[i][j]+=a[i][k]*b[k][j];
   }
  }
}
  for(int i=0;i<n;i++) {
    for (int j=0;j<w;j++) {
      cout<<c[i][j]<<" ";

    }
   cout<<endl; }}
void transpose(int matrix[][100], int N) {
    for (int i = 0; i < N; i++) {
      for (int j = i; j < N; j++) {
        // Swap elements at positions (i, j) and (j, i)
        swap(matrix[i][j], matrix[j][i]);
      }
    }
  }


void reverseRows(int matrix[][100], int N) {
    for (int i = 0; i < N; i++) {
      int start = 0, end = N - 1;
      while (start < end) {
        swap(matrix[i][start], matrix[i][end]);
        start++;
        end--;
      }
    }
  }


void rotateMatrix(int matrix[][100], int N) {

    transpose(matrix, N);

    reverseRows(matrix, N);
  }


void displayMatrix(int matrix[][100], int N) {
    for (int i = 0; i < N; i++) {
      for (int j = 0; j < N; j++) {
        cout << matrix[i][j] << " ";
      }
      cout << endl;
    }
  }
